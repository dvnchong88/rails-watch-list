<div class="container">
  <div class="cards">
    <h1 class="mt-3">Devin's Movies Watch List Website</h1>
    <% @lists.each do |list| %>
    <div class="card-product">
      <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
      <div class="card-product-infos">
        <h2><%= list.name %></h2>
        <%= link_to "Open", list_path(list.id), class: "btn btn-primary" %>
      </div>
    </div>
    <% end %>
  </div>
    <%= link_to "Add List", new_list_path, class: "mt-3 btn btn-primary" %>
</div>
